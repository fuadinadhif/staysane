# Your backend API URL (e.g., your apps/api deployment on Vercel)
NEXT_PUBLIC_API_URL="https://your-api-domain.vercel.app/api/v1"

# NextAuth Configuration
# Generate a secure secret with: openssl rand -base64 32
NEXTAUTH_URL="https://your-web-domain.vercel.app"
NEXTAUTH_SECRET="generate-a-secure-random-secret-with-openssl"

# JWT Secret
JWT_SECRET_KEY="your_jwt_secret_key_here"

# Google OAuth
# Get credentials from: https://console.cloud.google.com/apis/credentials
AUTH_GOOGLE_ID="your-google-client-id.apps.googleusercontent.com"
AUTH_GOOGLE_SECRET="your-google-client-secret"

# Google Maps API
# Get API key from: https://console.cloud.google.com/google/maps-apis
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your_google_maps_api_key_here"
GOOGLE_MAPS_API_KEY="your_server_side_google_maps_api_key_here"

# Midtrans Payment Gateway
# Get credentials from: https://dashboard.midtrans.com/
NEXT_PUBLIC_MIDTRANS_CLIENT_KEY="your-midtrans-client-key"



